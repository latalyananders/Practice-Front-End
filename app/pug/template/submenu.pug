                +e.subtitle Компания
                +e.links
                    +e('a')(href="#").item О компании
                    +e('a')(href="#").item Офисы продаж
                    +e('a')(href="#").item Контактная информация
            +b.search
                +e('img')(src="../../assets/images/magnifier.png").icon
