+b.submenu-container
    +b.submenu.close-submenu#submenu
        +e.title Карта сайта
        +b.tabs
            +b.plane
                +e('img')(src="../../assets/images/plane.png").icon
                +e.title Авиабилеты
                +e.subtitle Услуги
                +e.links
                    +e('a')(href="#").item Аэроэкспресс
                    +e('a')(href="#").item Гостиницы
                    +e('a')(href="#").item Такси
